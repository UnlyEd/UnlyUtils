{
  "name": "@unly/utils",
  "version": "1.0.0-alpha.0",
  "description": "Common utils",
  "main": "index.js",
  "author": "unlyEd",
  "license": "MIT",
  "repository": "https://github.com/UnlyEd/utils.git",
  "bugs": {
    "url": "https://github.com/UnlyEd/utils.git/issues"
  },
  "homepage": "https://github.com/UnlyEd/utils",
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "lint": "eslint . --cache --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "babel lib --out-dir dist",
    "pretest": "yarn run lint",
    "preversion": "yarn run lint && yarn run test",
    "version": "git add package.json",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "release:beta": "npm publish --tag beta --access=public",
    "release:alpha": "npm publish --tag alpha --access=public",
    "release:latest": "npm publish --tag latest --access=public",
    "publish": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14",
    "jest": "23.6.0"
  },
  "files": [
    "dist",
    "lib",
    "index.js",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
